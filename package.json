{
	"name": "excursionista",
	"version": "0.4.0",
	"private": true,
	"author": "Emanuela Coutinho",
	"homepage": "https://excursionista.manucoutinho.com/",
	"repository": {
		"directory": "git",
		"url": "https://github.com/ManuCoutinho/excursionista"
	},
	"keywords": [
		"next.js",
		"node",
		"react",
		"trips",
		"excursionista",
		"unsplash",
		"pictures",
		"landscapes",
		"chakra-ui"
	],
	"engines": {
		"node": ">=16.20",
		"npm": "8.19.4"
	},
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
		"prepare": "husky install",
		"test": "jest --watch --transformIgnorePatterns 'node_modules/(?!swiper)/'",
		"coverage": "jest --coverage --silent --transformIgnorePatterns 'node_modules/(?!swiper)/'",
		"codegen": "graphql-codegen --config codegen.yml"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"eslint src --fix",
			"jest --transformIgnorePatterns 'node_modules/(?!swiper)/' --passWithNoTests --findRelatedTests "
		],
		"*.{json,md, yaml, yml}": [
			"prettier --write"
		]
	},
	"msw": {
		"workerDirectory": "public"
	},
	"dependencies": {
		"@chakra-ui/react": "^2.5.5",
		"@emotion/react": "^11.10.4",
		"@emotion/styled": "^11.10.4",
		"@rollbar/react": "^0.11.1",
		"axios": "^1.1.3",
		"framer-motion": "^10.12.12",
		"graphql-request": "^5.0.0",
		"next": "^13.0.0",
		"next-seo": "^6.0.0",
		"nprogress": "^0.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.8.0",
		"react-query": "^3.39.3",
		"rollbar": "^2.26.1",
		"swiper": "^8.4.4",
		"uuidv4": "^6.2.13"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.22.5",
		"@babel/preset-typescript": "^7.18.6",
		"@graphql-codegen/cli": "2.13.12",
		"@graphql-codegen/client-preset": "1.1.5",
		"@graphql-codegen/typescript": "^2.8.2",
		"@graphql-codegen/typescript-operations": "^2.5.7",
		"@graphql-codegen/typescript-resolvers": "^2.7.7",
		"@testing-library/dom": "^9.3.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^29.5.1",
		"@types/node": "^18.11.5",
		"@types/nprogress": "^0.2.0",
		"@types/react": "^18.0.23",
		"@types/react-icons": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.57.0",
		"babel-jest": "^29.2.2",
		"eslint": "^8.41.0",
		"eslint-config-next": "12.1.0",
		"eslint-config-prettier": "^8.4.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^8.0.1",
		"jest": "^29.2.2",
		"jest-environment-jsdom": "^29.2.2",
		"lint-staged": "^13.0.3",
		"mock-match-media": "^0.4.2",
		"msw": "^1.2.1",
		"next-router-mock": "^0.7.4",
		"prettier": "^2.7.1",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.4"
	}
}
